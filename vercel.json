{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/api/server.js"
    },
    {
      "src": "^/styles\\.css$",
      "dest": "/public/styles.css"
    },
    {
      "src": "^/script\\.js$",
      "dest": "/public/script.js"
    },
    {
      "src": "^/chatbot\\.js$",
      "dest": "/public/chatbot.js"
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
